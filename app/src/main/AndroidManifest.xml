<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emo.lkplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.emo.lkplayer.view.PlaybackActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.emo.lkplayer.view.NagizarActivity"
            android:label="@string/title_activity_nagizar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.emo.lkplayer.view.EqualizerActivity"
            android:label="@string/title_activity_equalizer"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
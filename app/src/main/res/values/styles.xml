<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="StateButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@drawable/custom_activedisable_button</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="ListTextAppearnceMedium" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">#fffcfc</item>
    </style>

    <style name="ListTextAppearnceSmall" parent="TextAppearance.AppCompat.Small">
        <item name="android:textColor">#ffffff</item>
    </style>

    <!--<style name="listItemBackgroundSelector" parent="AppTheme">-->
        <!--<item name="android:background">@drawable/list_item_background_selector</item>-->
    <!--</style>-->
</resources>
